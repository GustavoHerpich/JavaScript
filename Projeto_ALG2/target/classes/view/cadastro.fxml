<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nutricionista.telas.cadastro">
    <TextField fx:id="campoNome" layoutX="39.0" layoutY="30.0" promptText="Nome" />
    <ComboBox fx:id="campoSexo" layoutX="385.0" layoutY="30.0" prefWidth="150.0" promptText="Sexo" />
    <TextField fx:id="campoIdade" layoutX="386.0" layoutY="66.0" promptText="Idade" />
    <TextField fx:id="campoEmail" layoutX="216.0" layoutY="30.0" promptText="Email" />
    <TextField fx:id="campoCelular" layoutX="39.0" layoutY="66.0" promptText="Celular" />
    <TextField fx:id="campoEndereco" layoutX="216.0" layoutY="66.0" promptText="Endereço" />
    <Button layoutX="39.0" layoutY="111.0" mnemonicParsing="false" onAction="#addAlimentacao" text="+ Adicionar alimentação" />
    <Button layoutX="40.0" layoutY="152.0" mnemonicParsing="false" onAction="#addRemedio" text="+ Adicionar remédio" />
    <Button layoutX="39.0" layoutY="192.0" mnemonicParsing="false" onAction="#addRestricao" text="+ Adicionar restrição" />
    <Button layoutX="482.0" layoutY="309.0" mnemonicParsing="false" onAction="#criarCliente" text="Criar cliente" />
   <Text fx:id="quantidadeAlimentacao" layoutX="205.0" layoutY="128.0" strokeType="OUTSIDE" strokeWidth="0.0" />
   <Text fx:id="quantidadeRemedio" layoutX="205.0" layoutY="169.0" strokeType="OUTSIDE" strokeWidth="0.0" />
   <Text fx:id="quantidadeRestricao" layoutX="205.0" layoutY="209.0" strokeType="OUTSIDE" strokeWidth="0.0" />
</AnchorPane>
